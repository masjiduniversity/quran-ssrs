  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>CREATE TABLE #tmp_tbl
	(SurahID uniqueidentifier,
	 Sur_rev_loc nvarchar(MAX))
	  INSERT INTO #tmp_tbl
			 ( [SurahID], [Sur_rev_loc] )
	 SELECT
		[ID],
		[Surah_Revealed_Location_English]
	FROM dbo.Surah;

	UPDATE #tmp_tbl
	SET Sur_rev_loc = 'Madni'  
	WHERE Sur_rev_loc = 'Mecca'  

	SELECT
	  S.Surah_No
	  ,S.Surah_Name_English
	  ,S.Surah_Name_Arabic
	  ,t.Sur_rev_loc
	  ,S.Surah_Revealed_Location_Arabic
	  ,S.Surah_Quantity_Ayyat
	FROM
	  Surah S
	LEFT JOIN #tmp_tbl t on t.SurahID=S.Id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
