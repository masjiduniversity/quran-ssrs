  <DataSets>
    <DataSet Name="Quran_DataSet">
      <Query>
        <DataSourceName>Quran_DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@USERINPUT">
            <Value>=Parameters!USERINPUT.Value</Value>
          </QueryParameter>
          <QueryParameter Name="Revealed_location">
            <Value>inlist("Mekkah","Mecca")</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  QURAN_Surah_List.Surah_Name_Arabic_Full
  ,QURAN_Surah_List.Surah_Name_Arabic
  ,QURAN_Surah_List.Surah_Name_English_Roman
  ,Surah.Surah_Revealed_Location_English
  ,Surah.Surah_Revealed_Location_Arabic
  ,Surah.Surah_Quantity_Ayyat
  ,Surah.Surah_No
FROM
  Surah
  INNER JOIN QURAN_Surah_List
    ON Surah.Id = QURAN_Surah_List.Surah_Id
WHERE
Surah.Surah_Revealed_Location_English = (@USERINPUT)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
